<h2><%= link_to l(:label_rb_genericboard_plural), rb_genericboards_path %> &#187; <%= l(:label_rb_genericboard_new) %></h2>

<%= labelled_form_for @rb_genericboard do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>
<p>
  <%= f.submit l(:button_create) %>
  <%= f.submit l(:button_create_and_continue), :name => 'continue' %>
</p>
<% end %>